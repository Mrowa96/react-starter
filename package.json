{
  "name": "react-starter",
  "private": true,
  "version": "3.3.0",
  "description": "React starter for basic web applications.",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server",
    "build": "cross-env NODE_ENV=production webpack",
    "build:serve": "cross-env NODE_ENV=production webpack && npm run serve",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint:scripts": "eslint --ext .js --ext .ts --ext .tsx .",
    "lint:styles": "npx stylelint \"**/*.scss\"",
    "lint:formatting": "npx prettier -c \"*.{ts,tsx,js,json,md,yml}\"",
    "check:code": "concurrently \"npm run lint:formatting\" \"npm run lint:styles\" \"npm run lint:scripts\" \"npm run test\"",
    "check:build": "cross-env NODE_ENV=production ANALYZE_BUILD=1 webpack",
    "storybook": "start-storybook -p 6006",
    "serve": "serve -s -n -l 3002 ./dist"
  },
  "author": "Pawe≈Ç Mrowiec <contact@pawel-mrowiec.dev>",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.6",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "normalize.css": "^8.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-svg-inline": "^2.1.1"
  },
  "devDependencies": {
    "@mrowa96/eslint-config-react": "^1.4.0",
    "@storybook/react": "^5.3.19",
    "@testing-library/jest-dom": "^5.11.1",
    "@testing-library/react": "^10.4.7",
    "@testing-library/react-hooks": "^3.3.0",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^26.0.5",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "autoprefixer": "^9.8.5",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.2.0",
    "copy-webpack-plugin": "^6.0.3",
    "css-loader": "^3.6.0",
    "eslint": "^7.5.0",
    "eslint-loader": "^4.0.2",
    "favicons-webpack-plugin": "^3.0.1",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss": "^7.0.32",
    "prettier": "^2.0.5",
    "raw-loader": "^4.0.1",
    "react-test-renderer": "^16.13.1",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.2",
    "serve": "^11.3.2",
    "style-loader": "^1.2.1",
    "stylelint": "^13.6.1",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.18.0",
    "stylelint-webpack-plugin": "^1.2.3",
    "ts-jest": "^26.1.3",
    "ts-loader": "^8.0.1",
    "typescript": "^3.9.7",
    "typescript-transformer-jsx-remove-attributes": "^1.0.6",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
